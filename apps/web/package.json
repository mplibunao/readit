{
	"name": "web",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint:check": "TIMING=1 next lint --cache --cache-location .eslintcache --ignore-path ../../.eslintignore",
		"typecheck": "tsc",
		"format:fix": "prettier --config ../../prettier.config.cjs --ignore-path ../../.prettierignore --write 'src/**/*.{js,jsx,ts,tsx,md,mdx,graphql,css,scss,yaml,json}'",
		"format:check": "prettier --config ../../prettier.config.cjs --ignore-path ../../.prettierignore --list-different 'src/**/*.{js,jsx,ts,tsx,md,mdx,graphql,css,scss,yaml,json}'"
	},
	"dependencies": {
		"next": "13.0.0",
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"eslint-config-custom": "workspace:*",
		"eslint": "7.32.0",
		"tsconfig": "workspace:*",
		"@types/node": "^17.0.12",
		"@types/react": "^18.0.22",
		"@types/react-dom": "^18.0.7",
		"typescript": "^4.5.3"
	}
}
