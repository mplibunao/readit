	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"eslint-config-custom-server": "workspace:*",
		"@readit/prettier-config": "workspace:*",
		"@readit/tsconfig": "workspace:*",
		"@types/node": "^18.0.0",
		"eslint": "^8.26.0",
		"pino-pretty": "^10.0.0",
		"prettier": "2.8.0",
		"typescript": "^5.0.0"
	},
	"dependencies": {
		"pino": "8.7.0",
		"zod": "^3.20.2"
	},
